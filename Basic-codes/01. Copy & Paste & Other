'Select all
Cells.Select

'selects all cess under current selection, until there's data
Range(Selection, Selection.End(xlDown)).Select

'selects all cess under current selection, except last line
Range(Selection, Selection.End(xlDown).Offset(-1)).Select

'copy a seleciton
Selection.Copy
Range().Copy

'paste special
Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
    :=False, Transpose:=False

'default paste
ActiveSheet.Paste

Application.CutCopyMode = False

'Clears data, keeps formatting
Selection.ClearContents

Selection.AutoFill Destination:=Range("A68795:L68795"), Type:=xlFillDefault

'----------------------------------------------------------------------------------

'Open a workbook
Workbooks.Open("path name")

Windows("filename").Activate

'Input formula to cell
ActiveCell.Formula = ""

'save workbook
ActiveWorkbook.Save

'turn screen update off and on
With Application
    .EnableEvents = False
    .ScreenUpdating = False
    .DisplayAlerts = False
End With

With Application
    .EnableEvents = True
    .ScreenUpdating = True
    .DisplayAlerts = True
End With

'insert column
Columns("G:G").Select
Selection.Insert Shift:=xlToRight, CopyOrigin:=xlFormatFromLeftOrAbove

'filtering
ActiveSheet.Range().AutoFilter Field:=3, Criteria1:="<>0", Operator:=xlFilterValues

--------------------------------

'msg box with mkultiple lines

MsgBox "line1" & vbCrLf & _
        "line2" & vbCrLf & _
        "line3" & vbCrLf & _
        "line4"

'msgbox with Yes/No

answer = MsgBox("Do you want to run it all?", vbYesNo + vbQuestion, "Run all")

If answer = vbYes Then
    'do something
Else
    'do nothing
End If


---------------------------------

'Windows minimize

ActiveWindow.WindowState = xlMinimized
Windows("aaa.xlsx").WindowState = xlMinimized
